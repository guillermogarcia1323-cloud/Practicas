#include <stdio.h>

int main() {
    // Declarar e inicializar un arreglo bidimensional 3x3
    int a[3][3] = {

        10, 11, 12,
        20, 21, 22,
        30, 31, 32

    };


    // Imprimir el arreglo
    printf("\n Arreglo bidimensional: \n\n");
    for(int i = 0; i < 3; i++) {
            if(i > 0)
                printf ("  ----+------+----\n");

        for(int j = 0; j < 3; j++) {
            printf(" %3i ", a[i][j]);
            if(j<2)
                printf (" |");
        }
        printf("\n");
    }


    return 0;
}
